<h5>Locaux existants</h5>
<table class="table table-bordered">
  <thead><tr>
    <th>Id</th>
    <th>Capacité</th>
    <th>Nom</th>
    <th>Location</th>
    <th>Description</th>
    <th>Date de création</th>
    <th>Dernière mise à jour</th>
  </tr></thead>
  <% @rooms.each do |room| %>
      <tr>
        <td><%= room.id %></td>
        <td><%= room.capacity %></td>
        <td><%= room.name %></td>
        <td><%= room.location %></td>
        <td><%= room.description %></td>
        <td><%= room.created_at %></td>
        <td><%= room.updated_at %></td>
      </tr>
  <% end %>
</table><hr>