<h5>COMPOSITEURS EXISTANTS</h5>
<table class="table table-bordered">
  <thead><tr>
    <th>Id</th>
    <th>Nom du compositeur</th>
    <th>Date de création</th>
    <th>Dernière mise à jour</th>
  </tr></thead>
  <% @composers.each do |composer| %>
      <tr>
        <td><%= composer.id %></td>
        <td><%= composer.name %></td>
        <td><%= composer.created_at %></td>
        <td><%= composer.updated_at %></td>
      </tr>
  <% end %>
</table><hr>